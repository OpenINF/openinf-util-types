{
  "name": "@openinf/util-types",
  "version": "2.1.3",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "compile": "npm run compile-src && npm run compile-doc",
    "compile-src": "tsc --project ./",
    "compile-doc": "jsdoc2md --template README.hbs --files index.js > ./README.md",
    "test": "tape '*.test.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openinf/util-types.git"
  },
  "keywords": [
    "types",
    "typechecking",
    "utilities"
  ],
  "author": "The OpenINF Authors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/openinf/util-types/issues"
  },
  "homepage": "https://github.com/openinf/util-types#readme",
  "description": "Fundamental JavaScript type-related utilities",
  "devDependencies": {
    "@types/jsdom": "^16.2.6",
    "@types/tape": "^4.13.0",
    "jsdoc-to-markdown": "^6.0.1",
    "jsdom": "^16.4.0",
    "tape": "^5.1.1",
    "typescript": "^4.1.3"
  }
}
