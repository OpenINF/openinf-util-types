{
  "name": "@openinf/util-types",
  "version": "2.1.3",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "prepare": "npm run compile-src && npm run compile-doc",
    "compile-src": "tsc --project ./",
    "compile-doc": "jsdoc2md --template README.hbs --files index.js > ./README.md",
    "test": "tape '*.test.js'"
  },
  "repository": "openinf/util-types",
  "keywords": [
    "types",
    "typechecking",
    "utilities"
  ],
  "author": "The OpenINF Authors",
  "license": "MIT",
  "bugs": "https://github.com/openinf/util-types/issues",
  "homepage": "https://github.com/openinf/util-types#readme",
  "description": "Fundamental JavaScript type-related utilities",
  "devDependencies": {
    "@types/jsdom": "20.0.0",
    "@types/tape": "4.13.2",
    "jsdoc-to-markdown": "7.1.1",
    "jsdom": "20.0.0",
    "tape": "5.6.0",
    "typescript": "4.8.3"
  }
}
